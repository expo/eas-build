{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "sourceMap": true,
    "inlineSources": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": false,
    "declaration": true,
    "forceConsistentCasingInFileNames": true
  }
}
