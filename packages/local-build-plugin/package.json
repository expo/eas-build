{
  "name": "eas-cli-local-build-plugin",
  "version": "0.0.2",
  "description": "Tool for running EAS compatible builds on a local machine.",
  "main": "dist/main.js",
  "bin": {
    "eas-cli-local-build-plugin": "./bin/run"
  },
  "scripts": {
    "start": "yarn watch",
    "watch": "tsc --watch",
    "build": "tsc",
    "prepack": "rm -rf dist && tsc -p tsconfig.build.json"
  },
  "engines": {
    "node": ">=12.9.0"
  },
  "author": "Expo <support@expo.io>",
  "bugs": "https://github.com/expo/eas-build/issues",
  "license": "MIT",
  "dependencies": {
    "@expo/build-tools": "^0.1.49",
    "@expo/bunyan": "^4.0.0",
    "@expo/eas-build-job": "^0.2.23",
    "@expo/spawn-async": "^1.5.0",
    "@hapi/joi": "^17.1.1",
    "chalk": "^4.1.0",
    "expo-cli": "^4.3.2",
    "fs-extra": "^9.1.0",
    "lodash": "^4.17.21",
    "semver": "^7.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.9",
    "@types/hapi__joi": "^17.1.6",
    "@types/lodash": "^4.14.168",
    "@types/semver": "^7.3.4",
    "@types/uuid": "^8.3.0",
    "typescript": "^4.2.4"
  }
}
