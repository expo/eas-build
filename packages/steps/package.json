    "@types/node": "^18.11.18",
    "bunyan": "^1.8.15",
    "chokidar-cli": "^3.0.0",
    "eslint-plugin-async-protect": "^3.0.0",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "ts-mockito": "^2.6.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@expo/logger": "^0.0.26",
