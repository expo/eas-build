{
  "name": "@expo/steps",
  "type": "module",
  "version": "0.0.1",
  "main": "./dist/commonjs/index.cjs",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/commonjs/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "start": "yarn watch",
    "build": "./build.sh",
